{
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "useTabs": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSameLine": true,
    "plugins": [
        "prettier-plugin-astro"
    ],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}